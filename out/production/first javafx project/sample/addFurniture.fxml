<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="702.0" prefWidth="715.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AddFurnitureController">
   <children>
      <ImageView fx:id="WOODTYPE" fitHeight="701.0" fitWidth="767.0" opacity="0.8">
         <image>
            <Image url="@black.PNG" />
         </image>
      </ImageView>
      <Label layoutX="238.0" layoutY="17.0" text="Add New Furniture" textFill="#c32222">
         <font>
            <Font name="Bell MT" size="36.0" />
         </font>
      </Label>
      <Label layoutX="154.0" layoutY="66.0" prefHeight="31.0" prefWidth="515.0" text="Choose what type of furniture you want to add...">
         <font>
            <Font name="Bell MT" size="23.0" />
         </font>
      </Label>
      <Button fx:id="addsalon" layoutX="200.0" layoutY="108.0" mnemonicParsing="false" onAction="#addsalon" prefHeight="34.0" prefWidth="94.0" style="-fx-background-color: #E65100;" text="salon" textFill="#f8f8f8">
         <font>
            <Font name="Bell MT" size="18.0" />
         </font>
      </Button>
      <Button fx:id="addliving" layoutX="376.0" layoutY="150.0" mnemonicParsing="false" onAction="#addliving" prefHeight="34.0" prefWidth="110.0" style="-fx-background-color: #E65100;" text="living room" textFill="#f8f8f8">
         <font>
            <Font name="Bell MT" size="18.0" />
         </font>
      </Button>
      <ImageView layoutX="625.0" layoutY="16.0">
         <image>
            <Image url="@sof(1)_1.png" />
         </image>
      </ImageView>
      <Button fx:id="addbedroom" layoutX="268.0" layoutY="150.0" mnemonicParsing="false" onAction="#addbedroom" prefHeight="34.0" prefWidth="94.0" style="-fx-background-color: #E65100;" text="bedroom" textFill="#f8f8f8">
         <font>
            <Font name="Bell MT" size="18.0" />
         </font>
      </Button>
      <Button fx:id="addkitchen" layoutX="308.0" layoutY="108.0" mnemonicParsing="false" onAction="#addkitchen" prefHeight="34.0" prefWidth="110.0" style="-fx-background-color: #E65100;" text="kitchen" textFill="#f8f8f8">
         <font>
            <Font name="Bell MT" size="18.0" />
         </font>
      </Button>
      <Button fx:id="addentrance" layoutX="431.0" layoutY="108.0" mnemonicParsing="false" onAction="#addentrance" prefHeight="34.0" prefWidth="94.0" style="-fx-background-color: #E65100;" text="entrence" textFill="#f8f8f8">
         <font>
            <Font name="Bell MT" size="18.0" />
         </font>
      </Button>
      <ImageView fx:id="CLOSE" fitHeight="30.0" fitWidth="67.0" layoutX="10.0" layoutY="9.0" onMousePressed="#CLOSE" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@close-window(13).png" />
         </image>
      </ImageView>
      <Button layoutX="620.0" layoutY="133.0" mnemonicParsing="false" onAction="#browse" prefHeight="34.0" prefWidth="109.0" style="-fx-background-color: #000000;" text="Browse" textFill="#f8f8f8">
         <font>
            <Font name="Bell MT" size="18.0" />
         </font>
      </Button>
      <BorderPane fx:id="mainPane" layoutX="56.0" layoutY="195.0" prefHeight="494.0" prefWidth="638.0" />
      <ImageView fx:id="BACK" fitHeight="34.0" fitWidth="43.0" layoutX="710.0" layoutY="655.0" onMousePressed="#BACK" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@circled-right-2.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
